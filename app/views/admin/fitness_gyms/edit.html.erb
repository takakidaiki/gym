<%= form_with model: @fitness_gym,url: admin_fitness_gyms_path do |f| %>
<%= f.text_field :name %>
  <div class="check_box">
    <span>タグ</span>
    <%= f.collection_check_boxes(:tag_ids, Tag.all, :id, :name) do |tag| %>
     <div>
       <%= tag.label do %>
         <%= tag.check_box %>
         <%= tag.text %>
       <% end %>
     </div>
   <% end %>
  </div>
  <%= f.submit '投稿' %>
  <%= link_to '一覧', admin_fitness_gyms_path(f) %>
  <%= link_to '削除', admin_fitness_gym_path(f), method: :delete %>
<% end %>
